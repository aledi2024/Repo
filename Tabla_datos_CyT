CREATE TABLE `cell` (
>>   `id` int(11) NOT NULL DEFAULT '0',
>>   `country_id` varchar(2) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,   
>>   `row_id` varchar(20) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,      
>>   `indicator_id` varchar(20) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
>>   `year` int(11) DEFAULT NULL,
>>   `value` decimal(18,4) NOT NULL
>> ) ENGINE=InnoDB DEFAULT CHARSET=utf8;

LOAD DATA INFILE 'C:\GITHUB\Repo\Datos_paises_ciencia.csv'
INTO TABLE cell
FIELDS TERMINATED BY ','
ENCLOSED BY '"'
LINES TERMINATED BY '\n'
(id, country_id, row_id, indicator_id, year, value);

print  head ´cell´

